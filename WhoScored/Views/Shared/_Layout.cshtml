<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>    
    <link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/DT_bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/vertical-buttons.less")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/who-scored.less")" rel="stylesheet" type="text/css" />
    

    <link href="@Url.Content("~/Content/jquery.dataTables.css")" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cascadingDropDown.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>   
    <script src="@Url.Content("~/Scripts/bootstrap-modal.js")" type="text/javascript"></script>   
    <script src="@Url.Content("~/Scripts/bootstrap-progressbar.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.store.min.js")" type="text/javascript"></script>
    
    @RenderSection("JavaScript", required: false)
</head>
<body>
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container">
                <a class="brand" href="#">WhoScored</a>
                <ul class="nav">
                    <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("League", "Index", "League")</li>
                    <li>@Html.ActionLink("Migration", "Index", "Migration")</li>
                </ul>
                <ul class="nav secondary-nav">
                    <li>@Html.Partial("_LogOnPartial")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        @RenderBody()
        <footer>
            <p>&copy; BestSoft 2012</p>
        </footer>
    </div> <!-- /container --> 
</body>
</html>
